<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane prefHeight="850.0" prefWidth="400.0" style="-fx-background-color: #2c232d;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.Controller">
   <children>
      <Text fx:id="title" fill="WHITE" layoutX="125.0" layoutY="62.0" strokeType="OUTSIDE" strokeWidth="0.0" text="{MovieTitle}">
         <font>
            <Font size="30.0" />
         </font>
      </Text>
      <Button fx:id="Screenings" graphicTextGap="20.0" layoutX="20.0" layoutY="675.0" mnemonicParsing="false" onAction="#swapPage" style="-fx-pref-height: 100; -fx-pref-width: 360;" text="Go back" textAlignment="CENTER" />
      <Button fx:id="Ticket" graphicTextGap="20.0" layoutX="20.0" layoutY="565.0" mnemonicParsing="false" onAction="#swapPage" style="-fx-pref-height: 100; -fx-pref-width: 360;" text="See ticket" textAlignment="CENTER" />
      <TextField fx:id="textInput" layoutX="78.0" layoutY="126.0">
         <padding>
            <Insets bottom="10.0" left="50.0" right="50.0" top="10.0" />
         </padding>
      </TextField>
   </children>
</Pane>
