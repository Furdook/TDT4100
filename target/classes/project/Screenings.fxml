<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane prefHeight="850.0" prefWidth="400.0" style="-fx-background-color: #2c232d;" xmlns="http://javafx.com/javafx/13" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.Controller">
   <children>
      <Text fx:id="title" fill="WHITE" layoutX="20.0" layoutY="65.0" strokeType="OUTSIDE" strokeWidth="0.0" text="{MovieTitle}">
         <font>
            <Font size="30.0" />
         </font>
      </Text>
      <Button fx:id="screening1" layoutX="20.0" layoutY="100.0" mnemonicParsing="false" onAction="#pickScreening" style="-fx-pref-width: 175; -fx-pref-height: 100;" text="{Screening_1}" />
      <Button fx:id="screening2" layoutX="205.0" layoutY="100.0" mnemonicParsing="false" onAction="#pickScreening" style="-fx-pref-width: 175; -fx-pref-height: 100;" text="{Screening_2}" />
      <Button fx:id="screening3" layoutX="20.0" layoutY="209.0" mnemonicParsing="false" onAction="#pickScreening" style="-fx-pref-width: 175; -fx-pref-height: 100;" text="{Screening_3}" />
      <Button fx:id="screening4" layoutX="205.0" layoutY="209.0" mnemonicParsing="false" onAction="#pickScreening" style="-fx-pref-width: 175; -fx-pref-height: 100;" text="{Screening_4}" />
      <Button fx:id="Movies" layoutX="20.0" layoutY="675.0" mnemonicParsing="false" onAction="#swapPage" style="-fx-pref-width: 360; -fx-pref-height: 100;" text="Go back" />
   </children>
</Pane>
