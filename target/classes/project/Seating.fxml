<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Pane prefHeight="850.0" prefWidth="400.0" style="-fx-background-color: #2c232d;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.Controller">
   <children>
      <Text fx:id="title" fill="WHITE" layoutX="21.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" text="{MovieTitle}">
         <font>
            <Font size="30.0" />
         </font>
      </Text>
      <Button fx:id="Screenings" graphicTextGap="20.0" layoutX="20.0" layoutY="675.0" mnemonicParsing="false" onAction="#swapPage" style="-fx-pref-height: 100; -fx-pref-width: 360;" text="Go back" textAlignment="CENTER" />
      <Button fx:id="ticketBtn" disable="true" graphicTextGap="20.0" layoutX="20.0" layoutY="565.0" mnemonicParsing="false" onAction="#swapPage" style="-fx-pref-height: 100; -fx-pref-width: 360;" text="See ticket" textAlignment="CENTER" />
      <TextField fx:id="textInput" layoutX="20.0" layoutY="138.0" prefHeight="37.0" prefWidth="360.0">
         <padding>
            <Insets bottom="10.0" left="50.0" right="50.0" top="10.0" />
         </padding>
      </TextField>
      <Text fill="WHITE" layoutX="20.0" layoutY="133.0" strokeType="OUTSIDE" strokeWidth="0.0" text="How many seats?">
         <font>
            <Font size="16.0" />
         </font>
      </Text>
      <Line endX="256.0" layoutX="121.0" layoutY="195.0" startX="-100.0" stroke="WHITE" />
      <TextField fx:id="nameInput" layoutX="20.0" layoutY="223.0" prefHeight="37.0" prefWidth="360.0">
         <padding>
            <Insets bottom="10.0" left="50.0" right="50.0" top="10.0" />
         </padding>
      </TextField>
      <Text fill="WHITE" layoutX="20.0" layoutY="220.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name:">
         <font>
            <Font size="16.0" />
         </font>
      </Text>
      <Text fill="WHITE" layoutX="289.0" layoutY="268.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Press enter after filling feed">
         <font>
            <Font size="8.0" />
         </font>
      </Text>
   </children>
</Pane>
